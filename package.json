{
	"name": "@batterii/fake-query",
	"version": "1.1.0",
	"description": "Fake Objection query builder for unit tests",
	"keywords": [
		"objection",
		"fake",
		"mock"
	],
	"author": "Batterii, LLC",
	"license": "MIT",
	"repository": "github:Batterii/fake-query",
	"files": [
		"dist/lib",
		"dist/types"
	],
	"scripts": {
		"generate": "yo --local-only",
		"generate:class": "yo @batterii/ts-class  --local-only ",
		"generate:module": "yo @batterii/ts-module --local-only ",
		"generate:test": "yo @batterii/ts-test --local-only ",
		"lint": "eslint . --ext .ts",
		"lintf": "eslint . --ext .ts --fix",
		"prebuild": "rm -rf dist",
		"build": "tsc && tsc --project tsconfig-types.json",
		"test": "mocha",
		"test:unit": "mocha test/unit",
		"test:integration": "mocha test/integration",
		"test:build": "npm run build && mocha --config .mocharc-dist.yaml dist/test",
		"preversion": "npm run lint && npm run test:build",
		"prepublishOnly": "git push && git push --tags"
	},
	"main": "dist/lib/index.js",
	"types": "dist/types/index.d.ts",
	"devDependencies": {
		"@batterii/eslint-config-ts": "^3.0.0",
		"@batterii/generator-ts-class": "^0.2.0",
		"@batterii/generator-ts-module": "^0.2.0",
		"@batterii/generator-ts-test": "^0.1.2",
		"@types/chai": "^4.2.14",
		"@types/lodash": "^4.14.165",
		"@types/mocha": "^7.0.2",
		"@types/node": "^10.17.44",
		"@types/sinon": "^7.5.2",
		"@types/sinon-chai": "^3.2.5",
		"@typescript-eslint/eslint-plugin": "^4.7.0",
		"@typescript-eslint/parser": "^4.7.0",
		"chai": "^4.2.0",
		"eslint": "^6.8.0",
		"knex": "^0.20.15",
		"mocha": "^7.2.0",
		"objection": "^2.2.3",
		"sinon-chai": "^3.5.0",
		"source-map-support": "^0.5.19",
		"ts-node": "^8.10.2",
		"typescript": "^4.0.5",
		"yo": "^3.1.1"
	},
	"dependencies": {
		"lodash": "^4.17.20",
		"sinon": "^7.5.0"
	}
}
